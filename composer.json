{
    "name": "stoker/randomjokes",
    "type": "phpbb-extension",
    "description": "Random Jokes display a random joke on index. Configurable from ACP.",
    "homepage": "https://www.phpbb3bbcodes.com",
    "version": "1.0.4",
    "time": "2025-04-04",
    "license": "GPL-2.0-only",
    "authors": [
        {
            "name": "Stoker",
            "homepage": "https://www.phpbb3bbcodes.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.4",
        "composer/installers": "~1.0.0"
    },
    "extra": {
        "display-name": "Random Jokes",
        "soft-require": {
            "phpbb/phpbb": ">=3.3.0"
        },
        "version-check": {
             "host": "www.phpbb3bbcodes.com",
            "directory": "/versioncheck",
            "filename": "randomjokes_version.json",
            "ssl": true
        }
    }
}